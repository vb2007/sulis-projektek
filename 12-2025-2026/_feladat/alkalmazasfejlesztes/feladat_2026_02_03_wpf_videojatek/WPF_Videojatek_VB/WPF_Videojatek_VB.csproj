<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="background.jpg" />
    <None Remove="coal.png" />
    <None Remove="diamond.png" />
    <None Remove="gold.png" />
    <None Remove="icon.png" />
    <None Remove="iron.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="background.jpg" />
    <Resource Include="coal.png" />
    <Resource Include="diamond.png" />
    <Resource Include="gold.png" />
    <Resource Include="icon.png" />
    <Resource Include="iron.png" />
  </ItemGroup>

</Project>
